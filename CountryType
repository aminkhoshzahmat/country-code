    public function getCode(): string
    {
        return $this->getData(COUNTRY_CODE);
    }

    public function getNumber(): string
    {
        return $this->getData(COUNTRY_PHONE);
    }

    public function getName(): string
    {
        return $this->getData(COUNTRY_NAME);
    }

